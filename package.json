{
  "name": "codediff.js",
  "version": "0.1.0",
  "description": "JavaScript-based two-column diff with syntax highlighting",
  "repository": {
    "type": "git",
    "url": "https://github.com/danvk/codediff.js.git"
  },
  "targets": {
    "default": {
      "context": "browser",
      "outputFormat": "global",
      "source": "src/codediff.ts",
      "isLibrary": true
    }
  },
  "dependencies": {
    "highlight.js": "^11.6.0",
    "jquery": ">=2.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "assert": "*",
    "grunt": "~0.4.0",
    "grunt-cli": "0.1.6",
    "grunt-contrib-qunit": "~0.5.1",
    "grunt-qunit-cov": "~0.3.6",
    "grunt-qunit-junit": "~0.1.0",
    "parcel": "^2.7.0",
    "phantomjs": "~1.9.7",
    "prettier": "^2.7.1",
    "qunitjs": "^1.14.0",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build"
  }
}
